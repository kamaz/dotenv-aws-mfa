{
  "name": "dotenv-aws-mfa",
  "description": "Helps to maintain MFA token in dotenv file",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kamaz/dotenv-aws-mfa"
  },
  "bugs": {
    "url": "https://github.com/kamaz/dotenv-aws-mfa/issues"
  },
  "keywords": [
    "aws",
    "token",
    "mfa",
    "authentication",
    "dotenv",
    "nodejs",
    "profile"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "prerelease": "git-authors-cli && git add .",
    "semantic-release": "semantic-release",
    "release": "standard-version -a",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "aws-sdk": "^2.1267.0",
    "chalk": "^5.1.2",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "inquirer": "^9.1.4",
    "ramda": "^0.28.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/ini": "^1.3.30",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.10",
    "@types/ramda": "^0.28.20",
    "@types/rc": "^1.2.1",
    "git-authors-cli": "^1.0.44",
    "git-cz": "^4.9.0",
    "jest": "^29.3.1",
    "semantic-release": "^19.0.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "contributors": [
    "Kamaz <kamaz@xsimple.org>",
    "Dan <devbydan@gmail.com>"
  ],
  "version": "0.0.6-0"
}
